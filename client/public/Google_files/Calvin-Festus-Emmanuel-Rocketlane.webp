/* Container for the video section */
.video-container {
    text-align: center;
    padding: 20px;
    max-width: 1200px;
    margin: 0 auto;
}

/* Title Style */
.video-container h2 {
    font-family: 'Poppins', sans-serif;
    font-size: 32px;
    color: #273F52;
    margin-bottom: 20px;
}

/* Video Wrapper */
.video-wrapper {
    position: relative;
    display: inline-block;
    width: 100%;
    max-width: 800px;
    margin-bottom: 20px;
}

/* Responsiveness */
.video {
    width: 100%;
    height: 450px;
    border-radius: 8px;
}

/* Video Description */
.video-description {
    position: absolute;
    bottom: 10px;
    left: 10px;
    right: 10px;
    padding: 15px;
    background-color: rgba(0, 0, 0, 0.6);
    border-radius: 8px;
    color: #fff;
}

.video-description h3 {
    font-family: 'Poppins', sans-serif;
    font-size: 20px;
    margin-bottom: 5px;
}

.video-description p {
    font-family: 'Poppins', sans-serif;
    font-size: 14px;
}

/* Media Queries for Responsiveness */
@media (max-width: 768px) {
    .video-wrapper {
        max-width: 100%;
    }

    .video {
        height: 250px;
    }

    .video-description h3 {
        font-size: 16px;
    }
}


